{
  "name": "create-react-ts-web-starter",
  "version": "0.0.2",
  "main": "cmd/create-react-ts-web-starter/index.js",
  "bin": {
    "create-react-ts-web-starter": "cmd/create-react-ts-web-starter/index.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config build/webpack/webpack.dev.js",
    "start:headless": "cross-env NODE_ENV=production webpack serve --config build/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack build --config build/webpack/webpack.production.js",
    "test": "jest --detectOpenHandles",
    "test:e2e:prepare": "npx playwright install-deps && npx playwright install",
    "test:e2e": "playwright test",
    "test:ci": "yarn test --runInBand --coverage --ci",
    "coverage:upload": "codecov -f coverage/*.json",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint -- --fix",
    "lint:css": "stylelint \"**/*.{css}\" --allow-empty-input",
    "lint:css:fix": "yarn lint:css -- --fix",
    "lint:ci": "yarn lint && yarn lint:css && yarn prettier:ci",
    "prettier": "prettier 'build/**' 'configs/**' 'src/**' 'e2e/**' --write --ignore-unknown",
    "prettier:ci": "yarn prettier --check",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "standard-version"
  },
  "keywords": [
    "react",
    "redux",
    "template",
    "boilerplate",
    "typescript",
    "front-end"
  ],
  "homepage": "https://github.com/vitorsalgado/react-web-starter/blob/master/README.md",
  "bugs": "https://github.com/vitorsalgado/react-web-starter/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:vitorsalgado/react-web-starter.git"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "email": "vsalgadopb@gmail.com",
    "url": "https://github.com/vitorsalgado"
  },
  "license": "MIT",
