{
  "name": "create-react-ts-web-starter",
  "version": "3.0.1",
  "main": "bin/create-react-ts-web-starter/index.js",
  "bin": {
    "create-react-ts-web-starter": "bin/create-react-ts-web-starter/index.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config build/webpack/webpack.dev.ts",
    "start:headless": "cross-env NODE_ENV=production webpack serve --config build/webpack/webpack.dev.ts",
    "build": "cross-env NODE_ENV=production webpack build --config build/webpack/webpack.production.ts",
    "test": "jest --detectOpenHandles",
    "test:e2e:prepare": "npx playwright install-deps && npx playwright install",
    "test:e2e": "playwright test",
    "test:ci": "npm test --runInBand --coverage --ci",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint --fix",
    "lint:css": "stylelint \"**/*.{css}\" --allow-empty-input",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:ci": "npm run lint && npm run lint:css && npm run fmt:ci",
    "fmt": "prettier 'build/**' 'config/**' 'src/**' 'e2e/**' --write --ignore-unknown",
    "fmt:ci": "npm run fmt --check",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "standard-version"
  },
  "keywords": [
    "react",
    "redux",
    "template",
    "boilerplate",
    "typescript",
    "front-end",
    "clean-architecture",
    "cli",
    "starter"
  ],
  "homepage": "https://vitorsalgado.github.io/react-web-starter/",
  "bugs": "https://github.com/vitorsalgado/react-web-starter/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:vitorsalgado/react-web-starter.git"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "email": "vsalgadopb@gmail.com",
    "url": "https://github.com/vitorsalgado"
  },
  "license": "MIT",
